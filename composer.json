{
  "name": "gavinz8/swoft-soft-delete",
  "type": "library",
  "keywords": [
    "swoole",
    "swoft",
    "swoft-component"
  ],
  "description": "Swoft/SoftDelete component for swoft framework",
  "license": "Apache-2.0",
  "require": {
    "php": ">7.1",
    "swoft/framework": "~2.0.0"
  },
  "autoload": {
    "psr-4": {
      "Swoft\\SoftDelete\\": "src/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^7.5"
  },
  "autoload-dev": {
    "psr-4": {
      "Swoft\\SoftDeleteTest\\Unit\\": "test/unit/",
      "Swoft\\SoftDeleteTest\\Testing\\": "test/testing/"
    }
  },
  "scripts": {
    "test": "./vendor/bin/phpunit -c phpunit.xml"
  }
}
